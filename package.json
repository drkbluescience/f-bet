    "@types/react-native": "~0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
