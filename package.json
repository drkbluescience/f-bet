{
  "name": "f-bet-mobile",
  "version": "1.0.0",
  "description": "Football betting platform mobile app",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "test": "jest",
    "test:system": "node -e \"require('./src/utils/setupTests.ts').runAllTests().then(r => console.log(r))\"",
    "test:health": "node -e \"require('./src/utils/setupTests.ts').quickHealthCheck().then(r => console.log(r))\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "sync:all": "node -e \"require('./src/services/dataSyncService.ts').DataSyncService.syncAll().then(r => console.log(r))\"",
    "sync:today": "node -e \"require('./src/services/dataSyncService.ts').DataSyncService.syncTodayFixtures().then(r => console.log(r))\"",
    "db:create": "node src/scripts/createTablesSimple.js",
    "db:sync": "node src/scripts/syncDataSimple.js",
    "db:setup": "npm run db:create && npm run db:sync",
    "db:test": "node src/scripts/simpleTest.js",
    "db:check": "node src/scripts/checkData.js",
    "db:sync-today": "node src/scripts/syncTodayFixtures.js",
    "db:add-fixtures": "node src/scripts/addSampleFixtures.js",
    "test:fixture-details": "node src/scripts/testFixtureDetails.js",
    "db:create-tracking": "node src/scripts/createDataTrackingTable.js",
    "test:data-usage": "node src/scripts/testDataUsageStats.js"
  },
